# Prime Sieve - count primes under 10000
let limit = 10000
let count = 0

fn isPrime(n) {
    if (n < 2) {
        return 0
    }
    for (let i = 2; i * i <= n; i = i + 1) {
        if (n % i == 0) {
            return 0
        }
    }
    return 1
}

for (let i = 2; i < limit; i = i + 1) {
    if (isPrime(i) == 1) {
        count = count + 1
    }
}

print(count)
